.container {
  padding-bottom: 30rpx;
}

.header {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.header .name-wrapper {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%;
  transform: translateY(-25%);
}

.header .banner {
  height: 100rpx;
  width: 100%;
  background: #ffcd33;
}

.header .avatar {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}

.header .center-text {
  box-sizing: border-box;
  height: 50rpx;
  line-height: 50rpx;
  width: 100%;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #646464;
}

.header .org {
  padding-top: 10rpx;
  font-size: 30rpx;
  font-weight: 600;
}

.header .name {
  padding-bottom: 10rpx;
  font-size: 26rpx;
}

.license {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin-top: 20rpx;
}

.license .item {
  position: relative;
  width: 32.5%;
  margin-top: 10rpx;
  text-align: center;
}

.license .pic {
  width: 100%;
  height: 190rpx;
  vertical-align: middle;
}

.license .item .text {
  margin-top: 12rpx;
  font-size: 26rpx;
  color: #333;
}

.license .item .text .audit {
  position: absolute;
  right: 10rpx;
  bottom: 30rpx;
  width: 46rpx;
  height: 46rpx;
}

.license .item .audit-text {
  color: #ffcc34;
}

.license .item .pass-text {
  color: #28e07a;
}

.license .item .nopass-text {
  color: #ff5744;
}

.flex {
  display: flex;
  margin-top: 30rpx;
  overflow: hidden;
}

.flex .flex-item {
  flex: 1;
  text-align: center;
}

.flex-item .icon-one {
  width: 52rpx;
  height: 52rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}

.flex-item .icon-two {
  width: 66rpx;
  height: 62rpx;
  margin-right: 20rpx;
  vertical-align: middle;
}

.flex-item .text {
  font-size: 40rpx;
  font-weight: 700;
  color: #333;
}

.wrapper {
  margin-top: 80rpx;
}

.wrapper .title {
  margin: 0 0 20rpx 26rpx;
  font-size: 44rpx;
  font-weight: 700;
  color: #333;
}

.wrapper .icon {
  float: right;
  width: 34rpx;
  height: 34rpx;
  padding: 12rpx 26rpx;
}

.purchases-list {
  max-height: 370rpx;
  overflow: hidden;
}

.wrapper .pic {
  width: 180rpx;
  height: 116rpx;
  font-size: 0;
  margin-right: 8rpx;
}

.wrapper .pic:nth-child(4n) {
  margin-right: 0;
}

.list {
  display: flex;
  max-height: 342rpx;
  overflow: hidden;
}

.list .item {
  width: 49.8%;
}

.list .item:first-child {
  margin-right: 1px;
}

.list .item .img {
  width: 100%;
  height: 200rpx;
}

.list .item .text {
  margin: 26rpx 0 0 26rpx;
  font-size: 0;
  color: #333;
}

.list .item .text-left {
  display: inline-block;
  margin-right: 30rpx;
  vertical-align: bottom;
  font-size: 30rpx;
}

.list .item .text-right {
  display: inline-block;
  width: 150rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 30rpx;
  color: #adadad;
}

.disin-item {
  margin-bottom: 50rpx;
}

.disin-item:last-child {
  margin-bottom: 0;
}

.disinfection {
  max-height: 432rpx;
  overflow: hidden;
}

.disinfection .img {
  float: left;
  width: 372rpx;
  height: 210rpx;
  margin: 0 6rpx 6rpx 0;
}

.disinfection .img:nth-child(2n) {
  margin-right: 0;
}

.content {
  padding: 40rpx 26rpx 0;
}

.content .text {
  margin-bottom: 20rpx;
  font-size: 0rpx;
  color: #333;
}

.content .text:last-child {
  margin-bottom: 0;
}

.content .text-l {
  display: inline-block;
  width: 190rpx;
  font-size: 30rpx;
}

.content .text-r {
  display: inline-block;
  width: 500rpx;
  vertical-align: top;
  font-size: 28rpx;
  color: #adadad;
}

.desc {
  padding: 20rpx 26rpx;
  font-size: 30rpx;
  color: #333;
}

.zanwu-tishi {
  text-align: center;
  font-size: 30rpx;
  color: #333;
}

.license .same-style {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 190rpx;
  background-color: #e0e0e0;
}
.license .same-style .zhengjian-name {
  font-size: 28rpx;
  color: #9c9b9b;
  letter-spacing: 1px;
}
.license .same-style .weitian {
  font-weight: bold;
  margin-top: 10rpx;
}